{
    "name": "@template/backend",
    "version": "1.0.0",
    "description": "GraphQL TypeScript Prisma template",
    "main": "index",
    "scripts": {
        "gen": "rm -f ./src/generated/schema.d.ts && gql-gen --config schema.yml",
        "genp": "rm -f ./src/generated/prisma-schema.d.ts && gql-gen --config prismaschema.yml",
        "clear": "rm -rf ./node_modules && rm -rf ./BE/node_modules && rm -rf ./FE/node_modules",
        "playground": "ts-node ./src/scripts/postgraphilePlayground.ts",
        "patch": "git pull && npm version patch && git push",
        "build": "npx typescript --build && cp -R ./src/graphql/*.graphql ../../dist/graphql && cp -R ./src/prisma/*.graphql ../../dist/prisma && cp -R ./src/generated/*.graphql ../../dist/generated"
    },
    "bin": "dist/index.js",
    "author": "Konstantin Astapov",
    "license": "ISC",
    "dependencies": {
        "@types/lodash": "^4.14.149",
        "@types/nodemailer": "^6.2.0",
        "apollo-server": "^2.4.8",
        "apollo-server-express": "^2.4.8",
        "argv-parser": "^0.1.4",
        "axios": "^0.19.0",
        "body-parser": "^1.18.2",
        "compression": "^1.7.3",
        "dotenv": "^8.0.0",
        "express": "^4.16.3",
        "express-ws": "^4.0.0",
        "graphql": "^14.2.1",
        "graphql-custom-types": "^1.5.0",
        "graphql-import": "^0.7.1",
        "graphql-iso-date": "^3.6.1",
        "graphql-type-json": "^0.3.0",
        "lodash": "^4.17.15",
        "moment": "^2.22.2",
        "node-cache-promise": "^1.0.0",
        "nodemailer": "^6.1.1",
        "oauth2-server": "^3.0.1",
        "postgraphile": "^4.3.3",
        "postgraphile-plugin-connection-filter": "^1.0.1",
        "prisma-binding": "^2.3.15",
        "prisma-client-lib": "^1.34.5",
        "pug": "^2.0.3",
        "redis": "^2.8.0",
        "request": "^2.88.0",
        "typescript": "^3.2.2",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "@playlyfe/gql": "2.6.1",
        "@pongsatt/graphql-schema-typescript": "^1.0.25",
        "@types/axios": "^0.14.0",
        "@types/compression": "0.0.36",
        "@types/graphql-iso-date": "^3.3.1",
        "@types/graphql-type-json": "^0.1.3",
        "@types/moment": "^2.13.0",
        "@types/node": "^12.0.4",
        "@types/redis": "^2.8.12",
        "@types/redux": "^3.6.0",
        "@types/redux-thunk": "^2.1.0",
        "@types/request": "^2.48.1",
        "@types/uuid": "^3.4.4",
        "graphql-binding": "^2.5.1",
        "graphql-code-generator": "^0.18.2",
        "graphql-codegen-typescript-common": "0.18.2",
        "graphql-codegen-typescript-template": "^0.18.2",
        "ts-node": "^8.2.0",
        "tsconfig-paths": "^3.8.0"
    }
}
